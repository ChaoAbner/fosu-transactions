    align-items: center;
}

.xinxi {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 15rpx;
    font-size: 36rpx;
    color: black;
}

.wctu {
    width: 150rpx;
    height: 150rpx;
    display: flex;
    border: 1rpx solid #c4c4c4;
    border-radius: 25%;
}

.stuid {
    padding: 10rpx 30rpx;
    font-size: 28rpx;
    border: 1rpx solid #c4c4c4;
    margin: 40rpx 80rpx 0rpx;
