/* pages/info/info.wxss */
@import "../feedback/feedback.wxss";

.weui-toptips {
    position: fixed;
    transform: translateZ(0);
    top: 0;
    left: 0;
    right: 0;
    padding: 5px;
    font-size: 14px;
    text-align: center;
    color: #FFFFFF;
    z-index: 5000;
    word-wrap: break-word;
    word-break: break-all;
}

.weui-toptips_warn {
    background-color: #E64340;
}